<h1 class="text-center mb-3">{{title}}</h1>

<h4>Group List</h4>
<hr>
<ul class="list-group">
  {{#each contact_groups}}
  <li class="list-group-item">
    <h4 style="text-transform: uppercase">{{this.id}}  {{this.group_name}}</h4>
    <a href="/groups/view/{{this.group_name}}"> viewContacts </a>
    <a href="/api/groups/edit/{{this.group_name}}"> Edit </a>
    <a href="/api/groups/delete/{{this.group_name}}"> Delete </a>
  </li>
  {{/each}}
</ul>
